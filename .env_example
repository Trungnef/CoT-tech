# API Keys

# GEMINI_API_KEYS là danh sách các API keys cho Gemini, phân tách bằng dấu phẩy
# Khi một key hết quotas, hệ thống sẽ tự động chuyển sang key tiếp theo
# Có thể lấy key mới từ Google AI Studio (https://ai.google.dev/)
GEMINI_API_KEYS=",YOUR_SECOND_GEMINI_KEY,YOUR_THIRD_GEMINI_KEY"

# OPENAI_API_KEYS là danh sách các API keys cho OpenAI, phân tách bằng dấu phẩy
OPENAI_API_KEYS=",YOUR_SECOND_OPENAI_KEY"

# GROQ_API_KEYS là danh sách các API keys cho Groq, phân tách bằng dấu phẩy
GROQ_API_KEYS=""

# Biến cũ để tương thích ngược (sẽ được thiết lập từ key đầu tiên trong danh sách)
GEMINI_API_KEY=""
OPENAI_API_KEY=""
GROQ_API_KEY=""

# Model Paths
QWEN_MODEL_PATH=""
QWEN_TOKENIZER_PATH=""

LLAMA_MODEL_PATH=""
LLAMA_TOKENIZER_PATH=""

# GPU Configuration
MAX_GPU_MEMORY_GB = 144
SYSTEM_RESERVE_MEMORY_GB=2.5
CPU_OFFLOAD_GB=24

# Vô hiệu hóa disk cache vì đã có đường dẫn trực tiếp đến model
ENABLE_DISK_CACHE=false  